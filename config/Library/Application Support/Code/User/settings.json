{
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDelete": false,
    "C_Cpp.clang_format_style": "{ BasedOnStyle: Google, ColumnLimit: 0, AllowShortFunctionsOnASingleLine: None }",
    "C_Cpp.updateChannel": "Insiders",
    "leetcode.workspaceFolder": "/Users/shota/git/github.com/ShotaKashihara/leetcode-swift",
    "leetcode.defaultLanguage": "swift",
    "leetcode.filePath": {
        "default": {
            "folder": "solutions",
            "filename": "${id}.${kebab-case-name}.${ext}"
        }
    },
    "leetcode.showDescription": "Both",
    "leetcode.hint.setDefaultLanguage": false,
    "leetcode.hint.commentDescription": false,
    "leetcode.hint.commandShortcut": false,
    "leetcode.hint.configWebviewMarkdown": false,
    "files.autoSave": "onFocusChange",
    "debug.onTaskErrors": "showErrors",
    "git.enableSmartCommit": true,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": false,
        "yaml": true,
        "plaintext": true,
        "markdown": false,
        "ini": false
    },
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.gpuAcceleration": "off",
    "window.zoomLevel": 1,
    "files.associations": {
        "*.swifttemplate": "swift"
    },
    "window.newWindowDimensions": "inherit",
    "files.trimTrailingWhitespace": true,
    "pasteImage.basePath": "${currentFileDir}/..",
    "pasteImage.path": "${currentFileDir}/../images",
    "pasteImage.insertPattern": "${imageSyntaxPrefix}/${imageFilePath}${imageSyntaxSuffix}"
}